                    <Grid x:Name="grid" Width="Auto" Height="Auto">
                        <Ellipse Stroke="#FF000000" x:Name="ellipse" StrokeThickness="1.5" Margin="3,3,3,3">
                            <Ellipse.Fill>
                                <RadialGradientBrush MappingMode="RelativeToBoundingBox">
                                    <GradientStop Color="#FF2FF012" Offset="0"/>
                                    <GradientStop Color="#FF071505" Offset="1"/>
                                    <GradientStop Color="#B22EEB11" Offset="0.51"/>
                                </RadialGradientBrush>
                            </Ellipse.Fill>
                        	<Ellipse.BitmapEffect>
                        		<DropShadowBitmapEffect Color="#FF2FF012"/>
                        	</Ellipse.BitmapEffect>
                        </Ellipse>
                        <ContentPresenter Opacity="0.595" OpacityMask="#FF59FF00" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" x:Name="contentPresenter" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" RecognizesAccessKey="True"/>
                        <Ellipse HorizontalAlignment="Stretch" Margin="3,3,3,3" x:Name="ellipse1" VerticalAlignment="Stretch" Width="Auto" Height="Auto" Stroke="#FF000000" StrokeThickness="1.5">
                            <Ellipse.Fill>
                                <RadialGradientBrush>
                                    <GradientStop Color="#00BBFF00" Offset="0.856"/>
                                    <GradientStop Color="#3BBBFF00" Offset="1"/>
                                </RadialGradientBrush>
                            </Ellipse.Fill>
                        </Ellipse>
                    	<Ellipse x:Name="ellipse1_Copy" Width="Auto" Height="Auto" StrokeThickness="1.5" Margin="5,5,5,5">
                    		<Ellipse.Fill>
                    			<LinearGradientBrush EndPoint="0.351,0.319" StartPoint="0.193,0.148">
                    				<GradientStop Color="#7FBDFFBB" Offset="0"/>
                    				<GradientStop Color="#00FFFFFF" Offset="1"/>
                    			</LinearGradientBrush>
                    		</Ellipse.Fill>
                    	</Ellipse>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Fill" TargetName="ellipse">
                                <Setter.Value>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#FF2FF012" Offset="0"/>
                                        <GradientStop Color="#FF071505" Offset="1"/>
                                        <GradientStop Color="#FFCFEB11" Offset="0.019"/>
                                    </RadialGradientBrush>
                                </Setter.Value>
                            </Setter>
                            <Setter Property="Fill" TargetName="ellipse1">
                                <Setter.Value>
                                    <RadialGradientBrush>
                                        <GradientStop Color="#00BBFF00" Offset="0.856"/>
                                        <GradientStop Color="#55CFEB11" Offset="1"/>
                                    </RadialGradientBrush>
                                </Setter.Value>
                            </Setter>
                            <Setter Property="BitmapEffect" TargetName="ellipse">
                                <Setter.Value>
                                    <DropShadowBitmapEffect Color="#76CFEB11" ShadowDepth="6" Softness="0.395"/>
                                </Setter.Value>
                            </Setter>
                        </Trigger>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Name="Timeline1_BeginStoryboard" Storyboard="{StaticResource Timeline1}"/>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard x:Name="Timeline2_BeginStoryboard" Storyboard="{StaticResource Timeline2}"/>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsFocused" Value="True">
                        	<Setter Property="Stroke" TargetName="ellipse" Value="#FF22FF00"/>
                        	<Setter Property="Stroke" TargetName="ellipse1" Value="#FF22FF00"/>
                        	<Setter Property="StrokeThickness" TargetName="ellipse" Value="2"/>
                        	<Setter Property="StrokeThickness" TargetName="ellipse1" Value="2"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
