<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2006" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
	x:Class="DIComplete.SplitButton"
	x:Name="SPL"
	Width="97" Height="30">

	<Grid x:Name="LayoutRoot" HorizontalAlignment="Left" VerticalAlignment="Top" Width="97" Height="30">
		<Grid HorizontalAlignment="Left" VerticalAlignment="Top" Width="96" Height="29.204">
			<ComboBox x:Name="InsetComboBox" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment" Margin="0,0,0,0.204" Style="{DynamicResource SimpleComboBox}" IsSynchronizedWithCurrentItem="True" SelectionChanged="InsetComboBox_SelectionChanged" Focusable="False">
				<ComboBoxItem Style="{DynamicResource SimpleComboBoxItem}" Content="Add"/>
				<ComboBoxItem Style="{DynamicResource SimpleComboBoxItem}" Content="Finish" Width="88" Height="15.96"/>
				<ComboBoxItem Style="{DynamicResource SimpleComboBoxItem}" Content="Back"/>
			</ComboBox>
			<Button x:Name="InsetButton" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment" IsTabStop="True" TabIndex="11" Focusable="True" KeyDown="InsetButton_KeyDown" Margin="0,0,19.948,0" Style="{DynamicResource SimpleButton}" Content="Add" Click="InsetButton_Click" />
		</Grid>
	</Grid>
</UserControl>